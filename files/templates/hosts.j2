[local]
localhost ansible_connection=local ansible_python_interpreter=/usr/bin/python3

[rancher_k8s_master]
{% for item in cluster_members.results : %}
{% if item.vms_item[1].role == "master" %}
{{item.name |replace("-", "_")}} ansible_connection=ssh ansible_host={{item.networkInterfaces[0].networkIP}} ansible_ssh_extra_args='-o StrictHostKeyChecking=no' ansible_ssh_private_key_file={{users.ssh[0].key_directory}}{{users.ssh[0].key_filename}} ansible_user={{users.ssh[0].username}} ansible_python_interpreter=/usr/bin/python3 
{% endif %}
{% endfor %}

